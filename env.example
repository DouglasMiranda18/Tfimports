# Arquivo de exemplo para variáveis de ambiente
# Copie este arquivo para .env e preencha com suas credenciais reais

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Mercado Pago Configuration
VITE_MERCADO_PAGO_PUBLIC_KEY=APP_USR-your_public_key_here
VITE_MERCADO_PAGO_ACCESS_TOKEN=APP_USR-your_access_token_here
VITE_MERCADO_PAGO_CLIENT_SECRET=your_client_secret_here
VITE_MERCADO_PAGO_SANDBOX=false

# Melhor Envio Configuration
VITE_MELHOR_ENVIO_TOKEN=your_melhor_envio_token_here

# Outras configurações de segurança
VITE_APP_ENV=development
VITE_API_URL=https://your-api-domain.com
